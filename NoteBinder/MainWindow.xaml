<Window x:Class="NoteBinder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoteBinder"
        mc:Ignorable="d"
        xmlns:views="clr-namespace:NoteBinder.Views"
        Title="{Binding DataContext.Title, ElementName=view,StringFormat={}{0} - NoteBinder}" Height="800" Width="1000">
    <Grid>
        <views:NoteBinderView  x:Name="view" />
    </Grid>
</Window>
